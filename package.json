{
  "version": "0.7.1",
  "license": "MIT",
  "main": "dist/index.js",
  "browser": "dist/browser.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "types"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --entry src/index.ts --entry src/browser.ts",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build --entry src/index.ts --entry src/browser.ts",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@tradersroom/partners",
  "author": "rnrnstar2",
  "module": "dist/partners.esm.js",
  "size-limit": [
    {
      "path": "dist/partners.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/partners.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.2.1"
  }
}
